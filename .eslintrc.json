{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "no-unused-vars": "off",
        "strict": "off",
        "react/jsx-filename-extension": "off",
        "jsx-a11y/anchor-is-valid": [
            "error",
            {
                "components": [
                    "Link"
                ],
                "specialLink": [
                    "to"
                ]
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-underscore-dangle": "off",
        "no-multi-assign": "off"
    }
}